    </owl:ObjectProperty>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#hasRightObject -->

    <owl:ObjectProperty rdf:about="&prv;hasRightObject">
        <rdfs:label xml:lang="en">object of right provision</rdfs:label>
        <rdfs:comment xml:lang="en">provision attribute which expresses the object of a right provision</rdfs:comment>
        <rdfs:domain rdf:resource="&prv;Right"/>
    </owl:ObjectProperty>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#hasStatusAddressee -->

    <owl:ObjectProperty rdf:about="&prv;hasStatusAddressee">
        <rdfs:label xml:lang="en">addressee of status provision</rdfs:label>
        <rdfs:comment xml:lang="en">provision attribute which indicates the addresse of a provision (status) giving powers or duties, so to adhere to a particular community</rdfs:comment>
