    </owl:ObjectProperty>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#hasViolationBearer -->

    <owl:ObjectProperty rdf:about="&prv;hasViolationBearer">
        <rdfs:label xml:lang="en">bearer of violation provision</rdfs:label>
        <rdfs:comment xml:lang="en">provision attribute which expresses the bearer of a violation provision</rdfs:comment>
        <rdfs:domain rdf:resource="&prv;Violation"/>
