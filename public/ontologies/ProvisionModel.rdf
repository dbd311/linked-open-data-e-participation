     -->

    


    <!-- http://lod-dk/ontologies/ProvisionModel#Attribution -->

    <owl:Class rdf:about="&prv;Attribution">
        <rdfs:label xml:lang="en">attribution</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prv;Constitutive"/>
        <rdfs:comment xml:lang="en">class of provisions attributing roles and powers</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#Constitutive -->

    <owl:Class rdf:about="&prv;Constitutive">
        <rdfs:subClassOf rdf:resource="&prv;Rule"/>
        <rdfs:comment xml:lang="en">class of provisions which introduce or assign a juridical profiles
to entities of a regulated reality</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#ContentAmendment -->

    <owl:Class rdf:about="&prv;ContentAmendment">
        <rdfs:label xml:lang="en">content amendment</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prv;RuleOnRule"/>
        <rdfs:comment xml:lang="en">class of amendment provisions which modify literally the content
of a norm, or their meaning without literal changes</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#Creation -->

    <owl:Class rdf:about="&prv;Creation">
        <rdfs:label xml:lang="en">creation</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prv;Constitutive"/>
        <rdfs:comment xml:lang="en">class of provisions introducing entities, in the sense that they create previously not-existent entities, of a regulated reality</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#Definition -->

    <owl:Class rdf:about="&prv;Definition">
        <rdfs:label xml:lang="en">definition</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prv;Constitutive"/>
        <rdfs:comment xml:lang="en">class of provisions defining and describing entities of a regulated reality</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#Deletion -->

    <owl:Class rdf:about="&prv;Deletion">
        <rdfs:label xml:lang="en">deletion</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prv;ContentAmendment"/>
        <owl:disjointWith rdf:resource="&prv;Insertion"/>
        <rdfs:comment xml:lang="en">Deletion of text</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#Duty -->

    <owl:Class rdf:about="&prv;Duty">
        <rdfs:label xml:lang="en">duty</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&prv;ExplicitDuty"/>
                    <rdf:Description rdf:about="&prv;ImplicitDuty"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&prv;RuleOnAction"/>
        <rdfs:comment xml:lang="en">class of provisions indicating the behaviour imposed by a norm</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#Establishment -->

    <owl:Class rdf:about="&prv;Establishment">
        <rdfs:label xml:lang="en">establishment</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prv;Creation"/>
        <rdfs:comment xml:lang="en">class of provisions establishing new institutional bodies</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#ExplicitDuty -->

    <owl:Class rdf:about="&prv;ExplicitDuty">
        <rdfs:label xml:lang="en">explicit duty</rdfs:label>
        <rdfs:label xml:lang="en">explicit right</rdfs:label>
        <owl:equivalentClass rdf:resource="&prv;ImplicitRight"/>
        <rdfs:subClassOf rdf:resource="&prv;Duty"/>
        <owl:disjointWith rdf:resource="&prv;ImplicitDuty"/>
        <rdfs:comment xml:lang="en">class of the Duties explicitly expressed in a legislative text</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#ExplicitLiability -->

    <owl:Class rdf:about="&prv;ExplicitLiability">
        <rdfs:label xml:lang="en">explicit liability</rdfs:label>
        <owl:equivalentClass rdf:resource="&prv;ImplicitPower"/>
        <rdfs:subClassOf rdf:resource="&prv;Liability"/>
        <owl:disjointWith rdf:resource="&prv;ImplicitLiability"/>
        <rdfs:comment xml:lang="en">class of the liabilities explicitly expressed in a legislative text</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#ExplicitPower -->

    <owl:Class rdf:about="&prv;ExplicitPower">
        <rdfs:label xml:lang="en">explicit power</rdfs:label>
        <owl:equivalentClass rdf:resource="&prv;ImplicitLiability"/>
        <rdfs:subClassOf rdf:resource="&prv;Power"/>
        <owl:disjointWith rdf:resource="&prv;ImplicitPower"/>
        <rdfs:comment xml:lang="en">class of the powers explicitly expressed in a legislative text</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#ExplicitRight -->

    <owl:Class rdf:about="&prv;ExplicitRight">
        <owl:equivalentClass rdf:resource="&prv;ImplicitDuty"/>
        <rdfs:subClassOf rdf:resource="&prv;Right"/>
        <owl:disjointWith rdf:resource="&prv;ImplicitRight"/>
        <rdfs:comment xml:lang="en">class of the Rights explicitly expressed in a legislative text</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#ExtensionAmendment -->

    <owl:Class rdf:about="&prv;ExtensionAmendment">
        <rdfs:label xml:lang="en">extension amendment</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prv;RuleOnRule"/>
        <rdfs:comment xml:lang="en">class of amendment provisions which extend or reduce
the cases on which the norm operates</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#ImplicitDuty -->

    <owl:Class rdf:about="&prv;ImplicitDuty">
        <rdfs:label xml:lang="en">implicit duty</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prv;Duty"/>
        <rdfs:comment xml:lang="en">class of the Duties not expressed in the text, in correlative correspondence with Rights</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#ImplicitLiability -->

    <owl:Class rdf:about="&prv;ImplicitLiability">
        <rdfs:label xml:lang="en">implicit liability</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prv;Liability"/>
        <rdfs:comment xml:lang="en">class of the Liabilities not expressed in the text, in correlative correspondence with Powers</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#ImplicitPower -->

    <owl:Class rdf:about="&prv;ImplicitPower">
        <rdfs:label xml:lang="en">implicit power</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prv;Power"/>
        <rdfs:comment xml:lang="en">class of the powers not expressed in the text, in correlative correspondence with liabilities</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#ImplicitRight -->

    <owl:Class rdf:about="&prv;ImplicitRight">
        <rdfs:label xml:lang="en">implicit right</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prv;Right"/>
        <rdfs:comment xml:lang="en">class of the Rights not expressed in the text, in correlative correspondence with Duties</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#Insertion -->

    <owl:Class rdf:about="&prv;Insertion">
        <rdfs:label xml:lang="en">insertion</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prv;ContentAmendment"/>
        <rdfs:comment xml:lang="en">Insertion of text</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#Liability -->

    <owl:Class rdf:about="&prv;Liability">
        <rdfs:label xml:lang="en">liability</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&prv;ExplicitLiability"/>
                    <rdf:Description rdf:about="&prv;ImplicitLiability"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&prv;Attribution"/>
        <rdfs:comment xml:lang="en">class of provisions establishing a reduction or extension of the rights of a subject</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#Organization -->

    <owl:Class rdf:about="&prv;Organization">
        <rdfs:label xml:lang="en">organization</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prv;Creation"/>
        <rdfs:comment xml:lang="en">class of provisions which discipline the organization of new institutional bodies</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#Permission -->

    <owl:Class rdf:about="&prv;Permission">
        <rdfs:label xml:lang="en">permission</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prv;RuleOnAction"/>
        <rdfs:comment xml:lang="en">class of provisions indicating behaviours allowed by competent authorities, even derogating from other provisions</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#Power -->

    <owl:Class rdf:about="&prv;Power">
        <rdfs:label xml:lang="en">power</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&prv;ExplicitPower"/>
                    <rdf:Description rdf:about="&prv;ImplicitPower"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&prv;Attribution"/>
        <rdfs:comment xml:lang="en">class of provisions establishing the possibility of a subject to produce specific juridical effects, namely to establish, modify or cancel a juridical relationship</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#Procedure -->

    <owl:Class rdf:about="&prv;Procedure">
        <rdfs:label xml:lang="en">procedure</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prv;Definition"/>
        <rdfs:comment xml:lang="en">class of provisions defining and describing procedures, namely the modality of compliance of a provision regulative of actions</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#Prohibition -->

    <owl:Class rdf:about="&prv;Prohibition">
        <rdfs:label xml:lang="en">prohibition</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prv;RuleOnAction"/>
        <rdfs:comment xml:lang="en">class of provisions expressing a duty in negative form</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#Proponent -->

    <owl:Class rdf:about="&prv;Proponent">
        <rdfs:label xml:lang="en">proponent</rdfs:label>
        <rdfs:comment xml:lang="en"> the user wich give ammendment</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#Provision -->

    <owl:Class rdf:about="&prv;Provision">
        <rdfs:label xml:lang="en">provision</rdfs:label>
        <rdfs:comment xml:lang="en">Class including all the provisions types (as TermDefinition, Procedure, Duty, Right, Power, as well as more technical ones as Insertion, Abrogation, Substitution, etc.) and related attributes (for example the Bearer of a Right, or the Definiendum of a Term Definition), reflecting the lawmaker directions. In the Provision Model, provision types are organised into two main groups: Rules (introducing entities or expressing deontic concepts) and Rules on Rules (different kinds of amendments). The values of provision attributes can be expressed by lexical units, or by concepts derived from thesauri/ontologies, able to provide additional information on the entities of the regulated domain. For each attribute both rdfs:domain and rdfs:range can be specified: rdfs:domain specifies the type of individuals a provision attribute applies to (e.g. the individuals of the class ExplicitDuty for a provision attribute hasExplicitDutyBearer); rdfs:range specifies the type of values of this provision attribute. Since legislative texts can deal with any aspects of the reality, the range of provision attributes modeled by OWL object properties, where not specified, may belong to any class of objects (individual of owl:Class). Note that this cannot be formally defined in the ontology in order to guarantee that the ontology is fully-compliant to the OWL 2 DL profile.</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#Redress -->

    <owl:Class rdf:about="&prv;Redress">
        <rdfs:label xml:lang="en">redress</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prv;Remedy"/>
        <rdfs:comment xml:lang="en">class of provisions that describe the effects of non-compliance with constitutive norms; the result is not a sanction or a punishment, for it is not a breach or violation of any obligation, nor an offence, but nullity</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#Regulative -->

    <owl:Class rdf:about="&prv;Regulative">
        <rdfs:subClassOf rdf:resource="&prv;Rule"/>
        <rdfs:comment xml:lang="en">class of provisions which discipline actions
or the substantial and procedural defaults (remedies)</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#Remedy -->

    <owl:Class rdf:about="&prv;Remedy">
        <rdfs:label xml:lang="en">remedy</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prv;Regulative"/>
        <rdfs:comment xml:lang="en">class of regulative provisions which discipline substantial and procedural defaults</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#Right -->

    <owl:Class rdf:about="&prv;Right">
        <rdfs:label xml:lang="en">right</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&prv;ExplicitRight"/>
                    <rdf:Description rdf:about="&prv;ImplicitRight"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&prv;RuleOnAction"/>
        <rdfs:comment xml:lang="en">class of provisions which give the power to act to protect or satisfy a specific interest recognized and protected by the legal system</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#Rule -->

    <owl:Class rdf:about="&prv;Rule">
        <rdfs:label xml:lang="en">rule</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prv;Provision"/>
        <rdfs:comment xml:lang="en">class of provisions introducing entities or expressing deontic concepts</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#RuleOnAction -->

    <owl:Class rdf:about="&prv;RuleOnAction">
        <rdfs:label xml:lang="en">rule on action</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prv;Regulative"/>
        <rdfs:comment xml:lang="en">class of regulative provisions which discipline actions</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#RuleOnRule -->

    <owl:Class rdf:about="&prv;RuleOnRule">
        <rdfs:label xml:lang="en">rule on rule</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prv;Provision"/>
        <rdfs:comment xml:lang="en">class of provisions introducing different kinds of amendments (content, temporal and extension amendments)</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#Status -->

    <owl:Class rdf:about="&prv;Status">
        <rdfs:label xml:lang="en">status</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prv;Attribution"/>
        <rdfs:comment xml:lang="en">class of provisions assigning powers and duties to an addressee with specific requirements,classifying him in a particular category of people or in more restricted community</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#Substitution -->

    <owl:Class rdf:about="&prv;Substitution">
        <rdfs:label xml:lang="en">substitution</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prv;Deletion"/>
        <rdfs:subClassOf rdf:resource="&prv;Insertion"/>
        <rdfs:comment xml:lang="en">substitution of text is the intersection betwee insertion and deletion, which means is the subclass of the both classes</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#TemporalAmendment -->

    <owl:Class rdf:about="&prv;TemporalAmendment">
        <rdfs:label xml:lang="en">temporal amendment</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prv;RuleOnRule"/>
        <rdfs:comment xml:lang="en">class of amendment provisions which modify the times of a norm
(come-into-force and efficacy times);</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#TermDefinition -->

    <owl:Class rdf:about="&prv;TermDefinition">
        <rdfs:label xml:lang="en">term definition</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prv;Definition"/>
        <rdfs:comment xml:lang="en">class of provisions defining and describing terms introducing entities of a regulated reality</rdfs:comment>
    </owl:Class>
    


    <!-- http://lod-dk/ontologies/ProvisionModel#Violation -->

    <owl:Class rdf:about="&prv;Violation">
        <rdfs:label xml:lang="en">violation</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prv;Remedy"/>
        <rdfs:comment xml:lang="en">class of provisions that describe the violation of a prohibition, typically linked to an ad-hoc penalty (or sanction)</rdfs:comment>
    </owl:Class>
    


